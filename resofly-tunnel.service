[Unit]
Description=ResoFly Cloudflare Tunnel & Emailer
After=network-online.target resofly.service
Wants=network-online.target

[Service]
User=team13
WorkingDirectory=/home/team13/RESOFLY/backend
ExecStart=/bin/bash start_tunnel_and_email.sh
Restart=on-failure
RestartSec=30

[Install]
WantedBy=multi-user.target
