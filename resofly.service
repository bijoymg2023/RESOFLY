[Unit]
Description=RESOFLY Service
After=network.target

[Service]
User=root
WorkingDirectory=/home/team13/RESOFLY/backend
ExecStart=/bin/bash start_backend.sh
KillMode=mixed
TimeoutStopSec=5
Restart=always
RestartSec=10
Environment=PORT=5000
Environment=CAMERA_SOURCE=proxy
# Add any other env vars here

[Install]
WantedBy=multi-user.target
